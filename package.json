{
  "name": "cozy-downloader",
  "version": "0.0.2",
  "description": "Download files from a given URL",
  "main": "server.js",
  "dependencies": {
    "americano": "0.4.4",
    "body-parser": "1.14.2",
    "cozydb": "0.1.7",
    "cozy-clearance": "0.1.22",
    "cozy-notifications-helper": "1.0.2",
    "express": "4.13.3",
    "morgan": "1.6.1"
  },
  "devDependencies": {
    "nodemon": "1.8.1"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "node_modules/.bin/nodemon server.js --ignore client/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SomeAverageDev/cozy-downloader.git"
  },
  "keywords": [
    "cozy",
    "downloader"
  ],
  "author": "SomeAverageDev",
  "cozy-permissions": {
    "Download": {
      "description": "Download description and required fields"
    }
  },
