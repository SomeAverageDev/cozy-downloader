{
  "name": "cozy-downloader",
  "version": "0.0.2",
  "description": "Download files from a given URL",
  "displayName": "Downloader",
  "author": "SomeAverageDev",
  "homepage": "https://github.com/SomeAverageDev/cozy-downloader",
  "main": "server.js",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SomeAverageDev/cozy-downloader/issues"
  },
  "icon-path": "icon.png",
  "dependencies": {
    "body-parser": "1.14.2",
    "cozydb": "0.1.7",
    "cozy-notifications-helper": "1.0.2",
    "express": "4.13.3",
    "morgan": "1.6.1"
  },
  "devDependencies": {
    "nodemon": "1.8.1"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "node_modules/.bin/nodemon server.js --ignore client/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SomeAverageDev/cozy-downloader.git"
  },
  "keywords": [
    "cozy",
    "downloader"
  ],
  "cozy-permissions": {
    "download": {
      "description": "Download and store files in your cozy"
    },
    "file": {
      "description": "Store files in your cozy"
    },
    "folder": {
      "description": "Store folders in your cozy"
    }, 
    "user": {
      "description": "Need information for the from part of email sent."
    },
    "send mail from user": {
      "description": "Send links by mail"
    },
    "send mail to user": {
      "description": "Send links by mail"
    },
    "cozyinstance": {
      "description": "Used to recover your language setting and your cozy's domain"
    }
  }
}
